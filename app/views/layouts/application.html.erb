<!DOCTYPE html>
<html>
  <head>
    <title>OdinKittens</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav>
      <%= link_to "Kittens", kittens_path %>
      <%= link_to "New Kitten", new_kitten_path %>
      <% flash.each do |type, message| %>
        <p><b><%=type.capitalize %>:</b> <%= message %></p>
      <% end %>
    </nav>
    <%= yield %>
  </body>
</html>
